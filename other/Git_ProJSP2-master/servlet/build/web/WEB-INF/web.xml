<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.shamim.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HttpServletHeaders</servlet-name>
        <servlet-class>com.shamim.HttpServletHeaders</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>com.urmi.servlet.Test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UrmiServlet</servlet-name>
        <servlet-class>com.urmi.servlet.UrmiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HttpServletHeaders</servlet-name>
        <url-pattern>/HttpServletHeaders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UrmiServlet</servlet-name>
        <url-pattern>/UrmiServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
